{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "src": "../src/Core/NE.Standard"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "index.md", "toc.yml", "articles/**.md", "api/**.yml" ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "NE.Standard",
      "_disableContribution": true
    },
    "template": [ "default", "modern", "custom" ]
  }
}
